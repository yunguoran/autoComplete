{
  "manifest_version": 3,
  "name": "autoComplete",
  "version": "1.0",
  "description": "Automatically complete username, password and token.",
  "permissions": ["storage"],
  "host_permissions": [""],
  "content_scripts": [
    {
      "matches": [""],
      "js": ["dist/content.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
